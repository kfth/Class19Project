{
    "extends": [
        "airbnb-base",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "func-names": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx"]
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
